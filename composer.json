{
  "name": "pega/patternlab_twig_extensions",
  "description": "Provides custom twig extensions.",
  "vendor": "pega",
  "type": "patternlab-plugin",
  "support": {
    "source": "http://vstash:7990/projects/LIB/repos/patternlab_twig_extensions.git"
  },
  "authors": [
    {
      "name": "Rob Barnett",
      "role": "Maintainer"
    }
  ],
  "autoload": {
    "psr-4": {"Pega\\TwigExtensions\\": "src/"}
  },
  "require": {
    "ksubileau/color-thief-php": "^1.3",
    "mexitek/phpcolors": "^0.4",
    "gregwar/image": "*"
  },
  "extra": {
    "patternlab": {
      "dist": {
        "sourceDir": [
          { "filters/*": "_twig-components/filters/*" },
          { "functions/*": "_twig-components/functions/*" },
          { "tags/*": "_twig-components/tags/*" },
          { "tests/*": "_twig-components/tests/*" }
        ]
      }
    }
  }
}
